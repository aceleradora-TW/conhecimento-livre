      <div class="playlist-container">
        <ul>
          {{#allContents}}
          <a href="/content/{{ _id }}">
            <li>{{ title }}</li>
          </a>
          {{/allContents}}
        </ul>
      </div>
  </div>
  </section>
</body>
</html>



<div>
